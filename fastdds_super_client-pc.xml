    export FASTRTPS_DEFAULT_PROFILES_FILE=/mnt/c/Users/thoma/Adeept_RaspClaws/fastdds_super_client-pc.xml

    C:\Users\thoma\Adeept_RaspClaws\fastdds_super_client-pc.xml
